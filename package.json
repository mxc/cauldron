{
  "name": "cauldron",
  "version": "1.0.0",
  "description": "A model, controller, view (MCV) framework for React",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mxc/cauldron.git"
  },
  "keywords": [
    "React"
  ],
  "author": "Mark Clarke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mxc/cauldron/issues"
  },
  "homepage": "https://github.com/mxc/cauldron#readme",
  "dependencies": {
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
